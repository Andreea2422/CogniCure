<script type="text/javascript" charset="utf-8">
    document.addEventListener("DOMContentLoaded", function () {
        const hardCodedKeywords = ['Anxiety', 'Addiction', 'ADHD', 'Bipolar Disorder', 'Breathing Techniques',
            'Cognitive Behavioral Therapy', 'Counselling', 'Depression', 'Eating Disorders',
            'Emotional Intelligence', 'Empathy', 'Food Guilt', 'Grief', 'Happiness', 'Loneliness',
            'Meditation', 'Mental Health', 'Mindfulness', 'Obsessive Compulsive Disorder', 'Online Bullying',
            'Personal Growth', 'PTSD', 'Self Love', 'Social Anxiety', 'Stress', 'Techniques To Use', 'Therapy'];
        const searchInput = document.querySelector(".search-bar");
        const autocompleteResults = document.querySelector(".autocomplete-results");

        function displayAutocompleteResults(inputValue) {
            // Clear previous results
            autocompleteResults.innerHTML = "";

            // Filter the hard-coded keywords based on the user's input
            const filteredKeywords = hardCodedKeywords.filter((keyword) =>
                keyword.toLowerCase().includes(inputValue.toLowerCase())
            );

            if (filteredKeywords.length > 0) {
                // Create an unordered list to hold the suggestions
                const span = document.createElement("span");
                const ul = document.createElement("ul");

                span.innerHTML = "Search Results";
                span.style.fontWeight = "900";
                span.style.display = "inline-block";
                span.style.margin = "5px 0 10px 5px";

                // Create and append list items for each suggestion
                filteredKeywords.forEach((keyword) => {
                    const li = document.createElement("li");
                    li.textContent = keyword;
                    li.addEventListener("click", function () {
                        searchInput.value = keyword;
                        autocompleteResults.style.display = "none";
                    });
                    ul.appendChild(li);
                });

                // Append the list to the autocomplete results container
                autocompleteResults.appendChild(span);
                autocompleteResults.appendChild(ul);
                autocompleteResults.style.display = "block";
            } else {
                autocompleteResults.style.display = "none";
            }
        }

        searchInput.addEventListener("input", function () {
            const inputValue = searchInput.value.trim();

            if (inputValue === "") {
                autocompleteResults.style.display = "none";
            } else {
                displayAutocompleteResults(inputValue);
            }
        });

    });

</script>